<div class="container">

  <h3 class="font-weight-light mb-5">Veterinarias</h3>


  {{#distritos}}

  <h5 class="display-5">{{distrito}}</h5>
  <hr class="mt-1 mb-4">

  <div class="row mb-5">
    {{#vets}}
    <div class="col-12 col-sm-6 col-md-6 col-lg-4 col-xl-3 my-1">
      <div class="card">
        <img src="{{image_url}}" class="card-img-top">
        <div class="card-body py-2">
          <div class="row justify-content-center">
            <a href="/vets/{{vet_id._id}}" class="btn btn-link">{{vet_id.name}} </a>
          </div>
        </div>
      </div>
    </div>
    {{/vets}}
  </div>


  {{/distritos}}

</div>